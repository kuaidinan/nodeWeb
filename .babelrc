{
  "presets": [
    ["env", { "modules": false }],
    "stage-2",
    "es2015"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime",
    ["component", [
      {
        "libraryName": "mint-ui",
        "style": true
      }
    ]]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
